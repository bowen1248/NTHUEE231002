#include<stdio.h>
int main(void)
{
	double mph; 
	double mps;
	printf("Input speed in miles per hour:\n");
	scanf("%lf\n",&mph);
	mps=mph*0.44704;
	printf("The speed in metric system is %lf meters per second.\n");
	return 0;
}
